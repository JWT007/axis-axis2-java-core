      <library name="lib/axis2-codegen-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axis2-common-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axis2-core-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axis2-java2wsdl-1.0.jar">
