   <runtime>
      <library name="Axis2CodegenWizard.jar">
         <export name="*"/>
      </library>
      <library name="lib/annogen-0.1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axiom-dom-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axiom-impl-1.0.jar">
         <export name="*"/>
      </library>
      <library name="lib/axis2-adb-1.0.jar">
         <export name="*"/>
